{
    "manifest_version": 2,
    "name": "Search with MyAnimeList",
    "version": "1.0",
    "description": "Search selected text on MyAnimeList in a new tab.",
    "icons": {
        "48": "icon.png"
    },
    "background": {
        "scripts": [
            "index.js"
        ]
    },
    "permissions": [
        "contextMenus",
        "menus",
        "tabs",
        "activeTab"
    ]
}